<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MFMeter">
        <attr name="meterTextSize" format="dimension" />
        <attr name="meterFontFamily" format="reference" />
        <attr name="meterFillBackground" format="boolean" />
        <attr name="meterSnapSessions" format="boolean" />
    </declare-styleable>


    <declare-styleable name="RoundedProgress">
        <attr name="rProgress" format="integer" />
        <attr name="rProgressColor" format="reference|color" />
        <attr name="rProgressGradientStart" format="reference|color" />
        <attr name="rProgressGradientEnd" format="reference|color" />
        <attr name="rProgressGradientAngle" format="integer" />
        <attr name="rBackgroundColor" format="reference|color" />
        <attr name="rTextColor" format="reference|color" />
        <attr name="rCornerRadius" format="dimension" />
        <attr name="rTextSize" format="dimension" />
        <attr name="rTextPaddingStart" format="dimension" />
        <attr name="rTextPaddingEnd" format="dimension" />
        <attr name="rFontFamily" format="reference" />
        <attr name="rTextShow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SleepStageGraph">
        <attr name="graphBackgroundColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="NapStageGraph">
        <attr name="napGraphBackgroundColor" format="reference|color" />
    </declare-styleable>
</resources>